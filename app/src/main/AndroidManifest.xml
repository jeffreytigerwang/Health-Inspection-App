<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.restaurantlist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Restaurant health inspection browsing"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".UI.inspectionDetailsActivity"
            android:label="@string/title_activity_inspection_details"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity android:name=".UI.restaurantDetailsActivity" />
        <activity
            android:name=".UI.ListActivity"
            android:label="Restaurant ListView" />
        <activity android:name=".UI.MenuActivity" />
        <activity android:name=".UI.welcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>